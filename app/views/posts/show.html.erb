<p id="notice"><%= notice %></p>

<p>
  <strong>Start:</strong>
  <%= @post.start.to_formatted_s(:long_ordinal) %>
</p>

<p>
  <strong>End:</strong>
  <%= @post.end.to_formatted_s(:long_ordinal) %>
</p>

<p>
  <strong>Start Location:</strong>
  <%= @post.start_loc %>
</p>

<p>
  <strong>End Location:</strong>
  <%= @post.end_loc %>
</p>

<p>
  <strong>Description:</strong>
  <%= @post.description %>
</p>

<p>
  <strong>Created By: </strong>
  <%= User.find(@post.user_id).user_name %>
</p>

<%= link_to "Contact Organizer", new_message_path(user_name: User.find(@post.user_id).user_name), class: "btn btn-success" %>

<%= link_to 'Back', posts_path, class: "btn btn-primary" %>

<% if session[:user_id] == @post.user_id %>
	<%= link_to 'Edit', edit_post_path(@post), class: "btn btn-warning" %>
<% end %>

